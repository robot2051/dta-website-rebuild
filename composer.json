    "homepage": "https://www.govcms.gov.au",
    "support": {
        "issues:": "https://github.com/govCMS/govCMS8/issues",
        "wik": "https://github.com/govCMS/govCMS8/wiki",
        "source": "https://github.com/govCMS/govCMS8/releases"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
          "type": "package",
          "package": {
            "name": "ckeditor/templates",
            "type": "drupal-library",
            "version": "4.5.7",
            "dist": {
              "type": "zip",
              "url": "https://download.ckeditor.com/templates/releases/templates_4.5.7.zip",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "govau/dta-uikit-base",
            "type": "drupal-custom-theme",
            "version": "dev-master",
            "source": {
              "type": "git",
              "url": "https://github.com/govau/dta-uikit-base",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "govau/dta-gov-au",
            "type": "drupal-custom-theme",
            "version": "dev-master",
            "source": {
              "type": "git",
              "url": "https://github.com/govau/dta-gov-au",
              "reference": "master"
            }
          }
        }
    ],
    "require": {
        "php": "~7.1.0",
        "bower-asset/dropzone": "^5.1",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.4",
        "drupal/admin_toolbar": "1.22",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/adminimal_theme": "^1.3",
        "drupal/allowed_formats": "^1.1",
        "drupal/backup_migrate": "^4.0@beta",
        "drupal/chosen": "^2.4",
        "drupal/chosen_lib": "^2.4",
        "drupal/config_split": "1.3",
        "drupal/consumers": "1.0-beta2",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/core": "^8.4",
        "drupal/disqus": "^1.0@RC",
        "drupal/dropzonejs": "2.0.0-alpha3",
        "drupal/ds": "^3.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_browser": "2.0-alpha2",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "1.4",
        "drupal/environment_indicator": "^3.2",
        "drupal/facets": "1.0-beta1",
        "drupal/features": "^3.5",
        "drupal/field_group": "^3.0",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/google_analytics": "^2.2",
        "drupal/govcms8_ui": "1.0.0-alpha1",
        "drupal/graphql": "3.0-beta2",
        "drupal/honeypot": "1.27",
        "drupal/imce": "^1.6",
        "drupal/inline_entity_form": "^1.0",
        "drupal/jsonapi": "1.7",
        "drupal/link_css": "1.x-dev",
        "drupal/linkit": "^5.0",
        "drupal/login_emailusername": "^1.1",
        "drupal/login_security": "^1.3",
        "drupal/memcache": "^2.0",
        "drupal/menu_block": "^1.4",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/metatag": "1.4.0",
        "drupal/module_filter": "^3.1",
        "drupal/node_title_help_text": "^1.0",
        "drupal/node_title_validation": "^1.1",
        "drupal/panelizer": "^4.0",
        "drupal/paragraphs": "^1.2",
        "drupal/password_policy": "^3.0",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "1.0.0",
        "drupal/restui": "^1.15",
        "drupal/robotstxt": "^1.1",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/search_api": "1.6",
        "drupal/search_api_autocomplete": "^1.0@beta",
        "drupal/search_api_page": "1.x-dev",
        "drupal/seckit": "^1.0",
        "drupal/shield": "^1.1",
        "drupal/simple_oauth": "3.4",
        "drupal/simple_sitemap": "2.11.0",
        "drupal/social_auth_google": "2.0-beta2",
        "drupal/swiftmailer": "^1.0",
        "drupal/toc_filter": "^2.0",
        "drupal/token": "1.1.0",
        "drupal/twig_tweak": "^1.9",
        "drupal/username_enumeration_prevention": "^1.0",
        "drupal/video_embed_field": "2.0-alpha2",
        "drupal/views_menu_children_filter": "^1.0",
        "drupal/webform": "5.0-rc1",
        "drupal/wysiwyg_template": "^2.0",
        "drush/drush": "~9.0",
        "govau/dta-gov-au": "dev-master",
        "govau/dta-uikit-base": "dev-master",
        "govau/dta-uikit-base-theme": "dev-master",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "require-dev": {
        "bex/behat-screenshot": "^1.2",
        "composer/composer": "^1.5",
        "dmore/behat-chrome-extension": "^1.1",
        "drupal/coder": "^8.2",
        "drupal/console": "^1.3",
        "drupal/devel": "^1.2",
        "drupal/drupal-extension": "^3.4",
        "grasmash/drupal-security-warning": "^1.1",
        "jakoch/phantomjs-installer": "^2.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": ">=4.8.36 <5",
        "se/selenium-server-standalone": "^3.6",
        "squizlabs/php_codesniffer": "^2.9"
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
          "drupal/node_title_validation": {
            "Unable to import configuration": "https://www.drupal.org/files/issues/2855289-7.patch",
            "Unique node title validation restricts node edit": "https://www.drupal.org/files/issues/unique_node_title-2828307-5.patch"
          }
        }
    },
    "autoload": {
        "psr-4": {
            "govCMS\\Core\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "conflict": {
        "drupal/core": "7.*",
        "drush/drush": "<8.1.10"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer package"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "package": "govCMS\\Core\\Composer\\Package::execute",
        "nuke": "rm -r -f bin docroot vendor"
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
